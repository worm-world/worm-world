{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "worm-world",
    "version": "0.1.1"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "fs": {
        "scope": ["**"]
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": ["icons/icon.png", "icons/icon.icns", "icons/icon.ico"],
      "identifier": "com.UofUBiology.WormWorld",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "focus": true,
        "visible": true,
        "maximized": false,
        "fullscreen": false,
        "height": 1000,
        "resizable": true,
        "title": "worm-world",
        "width": 1400
      }
    ]
  }
}
